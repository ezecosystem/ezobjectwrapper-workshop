<!DOCTYPE html>
<html lang="{{ app.request.locale }}">
<head>
    {% block page_head %}

        {% set meta_title = block('html_title') %}
        {% if meta_title is empty %}
            {% set meta_title = block('html_title_main') %}
        {% endif %}

        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=Edge">
        <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1"/>
        <title>{{ meta_title|raw }} - Kaliop</title>

        {% stylesheets output='compiled/styles'
        '@KaliopEzobjectWorkshopBundle/Resources/app/styles/bootstrap.min.css'
        '@KaliopEzobjectWorkshopBundle/Resources/app/styles/bootswatch.min.css'
        %}
        <link rel="stylesheet" href="{{ asset_url }}">
        {% endstylesheets %}

    {% endblock %}

</head>


<body>


    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">EzobjectWrapper Workshop</a>
            </div>
            <div id="navbar" class="collapse navbar-collapse">
                <ul class="nav navbar-nav">
                    <li class="active"><a href="/">Home</a></li>
                    <li><a href="#about">About</a></li>
                    <li><a href="#contact">Contact</a></li>
                </ul>
            </div><!--/.nav-collapse -->
        </div>
    </nav>



    {% block content %}{% endblock %}



{% javascripts output='compiled/scripts'
'@KaliopEzobjectWorkshopBundle/Resources/app/scripts/bootstrap.min.js'
%}
<script src="{{ asset_url }}"></script>
{% endjavascripts %}

</body>


</html>
